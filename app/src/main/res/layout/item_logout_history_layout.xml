<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="3dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/lightGreen"
        >


        <TextView
            android:id="@+id/item_logoutHistory_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:textColor="@color/black"
            android:text="TextView" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/item_logoutHistory_name"
            android:layout_margin="10dp"
            app:srcCompat="@drawable/ic_location_on_black_24dp" />

        <TextView
            android:id="@+id/item_logoutHistory_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_below="@+id/item_logoutHistory_name"
            android:textColor="@color/black"
            android:layout_toRightOf="@+id/imageView4"
            android:text="TextView" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView4"
            android:layout_margin="10dp"
            app:srcCompat="@drawable/ic_call_made_black_24dp" />


        <TextView
            android:id="@+id/item_logoutHistory_in"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:textColor="@color/black"
            android:layout_below="@+id/imageView4"
            android:layout_toRightOf="@+id/imageView5"
            android:text="TextView" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView5"
            android:layout_margin="10dp"
            app:srcCompat="@drawable/ic_call_received_black_24dp" />


        <TextView
            android:id="@+id/item_logoutHistory_out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:textColor="@color/black"
            android:layout_toRightOf="@+id/imageView6"
            android:layout_below="@+id/imageView5"
            android:text="TextView" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/item_logoutHistory_name"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/item_logoutHistory_location"
            app:srcCompat="@drawable/ic_perm_contact_calendar_black_24dp" />

        <TextView
            android:id="@+id/item_logoutHistory_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:textColor="@color/black"
            android:layout_toRightOf="@+id/imageView7"
            android:layout_below="@+id/item_logoutHistory_name"
            android:text="TextView" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/item_logoutHistory_id"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/item_logoutHistory_in"
            app:srcCompat="@drawable/ic_timer_black_24dp" />

        <TextView
            android:id="@+id/item_logoutHistory_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:textColor="@color/black"
            android:layout_marginLeft="10dp"
            android:layout_below="@+id/item_logoutHistory_id"
            android:layout_toRightOf="@+id/imageView8"
            android:text="TextView" />
    </RelativeLayout>

</androidx.cardview.widget.CardView>